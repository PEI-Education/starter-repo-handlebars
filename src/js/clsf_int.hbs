 {{#each students}}
  <div class="page">
    <header>
      <img src="./assets/cslf.jpg" id="cslf-logo">
      <img src="image/{{ school.id }}.jpg" id="school-logo">
      <div class="school">{{ school.name }}</div>
      <div class="schaddress">
        {{ school.address }}, {{ school.city }}, Île-du-Prince-Édouard, {{ school.postalcode }}<br />
        Tél: {{ school.phone }}  Fax: {{ school.fax }}
      </div>
      <div class="title">BULLETIN SCOLAIRE</div>
    </header>
<!-- student info -->     
    <div id="student_info" class="student-info">
      <div class="student1">
        NOM DE L'ÉLÈVE :<br>
        ADRESSE :<br>
        <br>
        TÉLÉPHONE :<br>
        PÉRIODE VISÉE :
      </div>
      <div class="student2">
        {{firstname}} {{lastname}}
        {{ street }}<br>
        {{ city }}, IPE, {{ postalcode }}<br>
        {{ phone }}
        {{ ../reportconfig.period }}
      </div>
      <div class="student3">
        No DE L'ÉLÈVE :<br>
        DATE DE NAISANCE :<br>
        NIVEAU SCOLAIRE :<br>
        <br>
        IMPRIMÉ LE :
      </div>
      <div class="student4">
        {{ number }}<br>
        {{ dob }}<br>
        {{ gradelevel }}<br>
        <br>
        {{ ../reportconfig.printDate }}
      </div>
    </div>

<!-- Courses -->
  {{#if courses.[0]}}
    {{#with courses.[0]}}
      <article class="course">
        <div class="course-info">
          Cours : {{ course_name }} <br>
          Code : {{ course_number }} <br>
          Enseignant.e : {{ teacher }} <br>
          <br>
        </div>
        <div class="attendance">
          Absences : {{ absences }} <br>
          Retards : {{ lates }}
        </div>
        <div class="q-m1-head">Étape 1</div>
        <div class="q-m2-head">Étape 2</div>
        <div class="q-m3-head">Étape 3</div>
        <div class="q-m4-head">Note Finale</div>
        <div class="q-m1-mark">{{ i1grade }}</div>
        <div class="q-m2-mark">{{ i2grade }}</div>
        <div class="q-m3-mark">{{ i3grade }}</div>
        <div class="q-m4-mark">{{ i4grade }}</div>
        <div class="comment">
          <strong><em>Commentaires :</em></strong>
            {{ comment }}
        </div>
      </article>
    {{/with}}
  {{/if}}
